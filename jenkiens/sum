pipeline{
    agent any
 parameters {
  string description: 'Enter Number1', name: 'num1'
  string description: 'Enter Number2', name: 'Num2'
}
stages{
    stage('add'){
        steps{
            sh'''
                  sum=$(($num1 + $Num2))
                  echo " Sum is: $sum " 
            '''
        }
    }
  }
}
