pipeline{
    agent any
   parameters {
  string description: 'enter name', name: 'name'
}

    stages{
        stage('build'){
            steps{
            build job: 'PassParam', parameters:[string(name: 'name',value: ${params.name})]
                sh '''
                echo "enter name:"
                
                echo " name is ${name}"
                '''
            }
        }
    }
}
